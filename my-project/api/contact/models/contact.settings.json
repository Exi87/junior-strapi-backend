{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "name": "Contact"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "ContactCoverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "ContactEmail": {
      "type": "email"
    },
    "ContactTel": {
      "type": "integer"
    },
    "ContactAddress": {
      "type": "string"
    },
    "ContactImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "FormName": {
      "type": "string"
    },
    "FormEmail": {
      "type": "email"
    },
    "FormMessage": {
      "type": "text"
    }
  }
}
